        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:QuizTime"
        mc:Ignorable="d"
        Title="vraagEdit" Height="700" Width="1400">
    <Grid HorizontalAlignment="Center" VerticalAlignment="Center" Margin="336,158,336,238">
        <TextBox Name="Vraag" HorizontalAlignment="Center" Height="23" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Center" Width="120" Margin="0,-3,0,3"/>
        <TextBox Name="AntwoordA" HorizontalAlignment="Center" Height="23" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Center" Width="120" Margin="0,25,0,-25"/>
