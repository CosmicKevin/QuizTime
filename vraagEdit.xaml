        <TextBox Name="AntwoordC" HorizontalAlignment="Center" Height="23" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Center" Width="120" Margin="0,81,0,-81"/>
        <TextBox Name="AntwoordD" HorizontalAlignment="Center" Height="23" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Center" Width="120" Margin="0,106,0,-106"/>
        <TextBox Name="Image" HorizontalAlignment="Center" Height="23" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Center" Width="120" Margin="0,186,0,-186"/>
        <TextBox Name="GoedAntwoord" HorizontalAlignment="Center" Height="23" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Center" Width="120" Margin="0,133,0,-133"/>
        <TextBox Name="Tijd" HorizontalAlignment="Center" Height="23" TextWrapping="Wrap" Text="30" VerticalAlignment="Center" Width="120" Margin="0,158,0,-158"/>
        <Button Name="btnSave" Content="Save" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="189,211,-144,-208"/>

    </Grid>
</Window>
